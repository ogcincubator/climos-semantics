@prefix climos: <https://w3id.org/climos/> .
@prefix climos-props: <https://w3id.org/climos/properties/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

climos:properties a skos:ConceptScheme ;
  skos:prefLabel "CLIMOS Properties" ;
  skos:definition "Properties and terms defined to support the CLIMOS ontology" ;
.

climos-props:usedInCLIMOS a skos:Concept ;
  skos:prefLabel "Used in CLIMOS" ;
  skos:definition "Declares whether this species, term or property has been used in CLIMOS." ;
  rdfs:range xsd:boolean ;
.